#NAME infer.

(; Example from 3.10 in bidirectional refinement article (page 21) ;)
Nat : Type.
P : Nat -> Type.
Q : n : Nat -> P n -> Type.

c1 : Nat.
c2 : P c1.
c3 : Q c1 c2.

T := x : Nat -> y : P x -> z : Q x y -> Nat.

#CHECK (f => f c1 c2 c3), (f:T -> Nat).

#CHECK (f => f c1 c2 c3), (f:?{"T"} -> Nat).

#INFER (f:? => P (f c1 c2 c3)).

#INFER (f:? => f (f c1)).
